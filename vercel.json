{
  "version": 2,
  "functions": {},
    "builds": [
      {
        "src": "backend/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/api/index.js"
      },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}